{
  "name": "gestao-financas-pessoais",
  "version": "1.0.0",
  "description": "Trabalho de conclusao de curso (Eduardo Sa Ribeiro de Moraes - Leonardo Aparecido Cardoso Villaslobo)",
  "main": "app/index.ts",
  "nodemonConfig": {
    "watch": [
      "app"
    ],
    "ext": "ts",
    "ignore": [
      "*.test.ts"
    ],
    "delay": "3",
    "execMap": {
      "ts": "ts-node"
    }
  },
  "scripts": {
    "test": "jest",
    "compile": "tsc",
    "prod": "ts-node dir/index.ts",
    "start": "nodemon --watch 'app' --ext ts --exec ts-node app/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GravetoGeek/gestao-financas-pessoais.git"
  },
  "keywords": [
    "FinTech"
  ],
  "author": "Eduardo Sa Ribeiro de Moraes",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GravetoGeek/gestao-financas-pessoais/issues"
  },
  "homepage": "https://github.com/GravetoGeek/gestao-financas-pessoais#readme",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "jsonwebtoken": "^9.0.0",
    "knex": ">=2.4.0",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.9.1",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.1.0",
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/faker": "^6.6.9",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/knex": "^0.16.1",
    "@types/node": "^17.0.35",
    "@types/nodemailer": "^6.4.7",
    "dotenv": "^16.0.1",
    "nodemon": "^2.0.20"
  }
}
